/* pages/Bproject/projectlist.wxss */
.searchwrap{
    background-color: #fff;
    padding: 20rpx;
}
.searchinner{
    height: 60rpx;
    line-height: 60rpx;
    border: 1px solid #ddd;
    border-radius: 10rpx;
}
.searchinput{
    min-width: 85%;
    height: 60rpx;
    line-height: 60rpx;
    border: none;
    padding: 0 10px;
    border-radius: 10rpx;
}
.searchimg{
    width: 40rpx;
    height: 40rpx;
    margin-top: 10rpx;
}
.tabbtnwrap{
    background-color: #fff;
}
.tabbtn {
    display: inline-block;
    width: 50%;
    text-align: center;
    padding: 10px 0;
    border-bottom: 1px solid #eee;
}

.tabbtn.on {
    color: #1e9fff;
    border-color: #1e9fff;
}

.noneproject {
    padding: 50px 0;
}

.addrightbtn {
    position: fixed;
    right: 20rpx;
    top: 70%;
}

.addbtnimg {
    width: 64rpx;
    height: 64rpx;
}

.prolist {
    margin-top: 20rpx;
}

.proimage {
    width: 100px;
    height: 100px;
}

.proright {
    margin-left: 15rpx;
}

.proright view {
    margin-top: 20rpx;
}

.probottom {
    margin-top: 30rpx;
}

.updatebtn {
    display: inline-block;
    width: 30%;
    text-align: center;
    color: #fff;
    padding: 10rpx 0;
    margin-left: 5%;
    background-color: #1e9fff;
}

.updatebtn:first-child {
    margin-left: 0;
}

.picon {
    width: 50rpx;
    height: 50rpx;
}
.prolist{
    padding: 10px 20rpx;
    background-color: #fff;
}

/*设置的弹窗设置*/

.florsetwrap {
    position: absolute;
    top: 50%;
    left: 50%;
    background-color: #000;
    width: 300rpx;
    height: 300rpx;
    padding: 50rpx;
    margin: -175rpx 0 0 -175rpx;
    border-radius: 30rpx;
    color: #fff;
    z-index: 200
}

.florimg {
    width: 50rpx;
    height: 50rpx;
}
.pulic{
    position: absolute;
    top: 50rpx;
    left: 50%;
    text-align: center;
    margin-left: -35rpx;
}
.pulic view{margin-top: 10rpx;}
.wx-switch-input{width:40px !important;height:20px !important;}
.wx-switch-input::before{width:38px !important;height: 18px !important;}
.wx-switch-input::after{width: 18px !important;height: 18px !important;}
.over{
    position: absolute;
    left: 50rpx;
    top: 50%;
    margin-top: -24rpx;
}
.delete{
    position: absolute;
    bottom: 50rpx;
    left: 50%;
    margin-left: -25rpx;
}
.update{
    position: absolute;
    right: 50rpx;
    top: 50%;
    margin-top: -24rpx;
}
.bottominner{
    padding: 30rpx 0;
}
.title{
    display: block;
    text-align: center;
    font-size: 30rpx;
    padding: 15rpx 0;
}
.buttons{
    margin-top: 10rpx;
}
.buttonwrap{
    position: relative;
    width: 25%;
    text-align: center;
}
.publicimg{
    display: block;
    width: 40rpx;
    height: 40rpx;
    margin: 10rpx auto;
}
.sharebtn{
    position: absolute;
    top: 0;
    left: 20rpx;
    width: 150rpx;
    height: 100rpx;
    opacity: 0;
}
.xiaochengxu{
    display: block;
    width: 258rpx;
    height: 258rpx;
    margin: 0 auto
}


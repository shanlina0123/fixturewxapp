/**index.wxss**/

.companyname {
    display: block;
    font-size: 36rpx;
    text-align: center;
    padding: 30rpx 0;
}

.buttonwrap {
    padding: 50rpx 0;
    background-color: #fff;
}

.centerbtn {
    display: inline-block;
    width: 31%;
    height: 80rpx;
    line-height: 80rpx;
    color: #fff;
    margin-left: 3.5%;
    text-align: center;
    background-color: #eb7221;
    border-radius: 5px;
}

.centerbtn:first-child {
    margin-left: 0;
}

.centertitle {
    display: block;
    font-size: 32rpx;
    padding: 20rpx;
}


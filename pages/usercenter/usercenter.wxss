/* pages/usercenter/usercenter.wxss */
.userwrap{
    position: relative;
    height: 190px;
}
.headbg{
    width: 100%;
    height: 160px;
}
.usermsgwrap{
    position: absolute;
    width: 100%;
    bottom: -50rpx;
    text-align: center;
}
.usermsgwrap image{
    width: 128rpx;
    height: 128rpx;
    border-radius: 128rpx;
    border: 6rpx solid #fff; 
}
.usermsgwrap view{
    font-size: 30rpx;
}
.proul{
    padding: 0 30rpx;
    margin-top: 90rpx;
}
.proul navigator{
    padding: 30rpx 0;
    border-top: 1px solid #eee;
}
.proul navigator text{
    line-height: 32rpx;
}
.itemicon{
    width: 45rpx;
    height: 45rpx;
    vertical-align: middle;
}
